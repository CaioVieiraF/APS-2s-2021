        android:background="#c0c0c0" />

    <TextView
        android:id="@+id/userToken"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:textColor="@color/black"
